<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.reedelk.plugin.builder.DownloadErrorPanel">
  <grid id="27dc6" binding="errorPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="empty">
      <size top="8" left="8" bottom="0" right="8"/>
    </border>
    <children>
      <grid id="3ade7" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="97423" class="javax.swing.JLabel" binding="errorTitleLabel">
            <constraints border-constraint="Center"/>
            <properties>
              <enabled value="true"/>
              <font style="1"/>
              <icon value="general/error.png"/>
              <text value="Download Runtime Distribution Error"/>
            </properties>
          </component>
          <component id="af3ff" class="javax.swing.JSeparator">
            <constraints border-constraint="South"/>
            <properties/>
          </component>
        </children>
      </grid>
      <grid id="b5b02" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="empty">
          <size top="8" left="8" bottom="0" right="0"/>
        </border>
        <children>
          <component id="f3f07" class="javax.swing.JLabel" binding="errorMessageLabel">
            <constraints border-constraint="Center"/>
            <properties>
              <text value="An error has occurred"/>
              <verticalAlignment value="1"/>
              <verticalTextPosition value="0"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
