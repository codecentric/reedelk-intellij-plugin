<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.reedelk.plugin.builder.SelectRuntimeSourceStep">
  <grid id="27dc6" binding="jPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="empty">
      <size top="0" left="8" bottom="0" right="8"/>
    </border>
    <children>
      <grid id="8d3e6" layout-manager="GridLayoutManager" row-count="3" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="b29a3" class="javax.swing.JRadioButton" binding="useRuntimeFromFileRadioButton" default-binding="true">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text resource-bundle="messages/ReedelkBundle" key="selectRuntimeStep.use.runtime.file.system"/>
            </properties>
          </component>
          <component id="8d10c" class="javax.swing.JRadioButton" binding="downloadLatestRuntimeRadioButton" default-binding="true">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <selected value="true"/>
              <text resource-bundle="messages/ReedelkBundle" key="selectRuntimeStep.download.latest"/>
            </properties>
          </component>
          <vspacer id="47e1d">
            <constraints>
              <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
            </constraints>
          </vspacer>
        </children>
      </grid>
      <grid id="a4f7d" binding="titlePanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="empty">
          <size top="8" left="0" bottom="8" right="0"/>
        </border>
        <children>
          <component id="bff19" class="javax.swing.JSeparator">
            <constraints border-constraint="South"/>
            <properties/>
          </component>
          <component id="dc257" class="javax.swing.JLabel">
            <constraints border-constraint="Center"/>
            <properties>
              <font/>
              <text value="Choose  Reedelk Runtime Distribution"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
